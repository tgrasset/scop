# Material defined in separate file.
mtllib cube.mtl

g cube

# Vertices
v 0.0 0.0 0.0
v 0.0 1.0 0.0  
v 1.0 1.0 0.0  
v 1.0 0.0 0.0  
v 0.0 0.0 1.0  
v 0.0 1.0 1.0  
v 1.0 1.0 1.0  
v 1.0 0.0 1.0  

# Texture
vt 0.25 1.00 
vt 0.50 1.00  
vt 0    0.75  
vt 0.25 0.75  
vt 0.50 0.75  
vt 0.75 0.75  
vt 0    0.50  
vt 0.25 0.50  
vt 0.50 0.50  
vt 0.75 0.50  
vt 0.25 0.25  
vt 0.50 0.25  
vt 0.25 0     
vt 0.50 0     

# Define material for the following faces
usemtl texture

# Face 1: cghd = cgh + chd
f 3 7 8
f 3 8 4

# Face 2: aefb = aef + afb
f 1 5 6
f 1 6 2

# Face 3: gcbf = gcb + gbf
f 7 3 2
f 7 2 6

# Face 4: dhea = dhe + dea
f 4 8 5
f 4 5 1

# Face 5: hgfe = hgf + hfe
f 8 7 6
f 8 6 5

# Face 6: cdab = cda + cab
f 3 4 1
f 3 1 2